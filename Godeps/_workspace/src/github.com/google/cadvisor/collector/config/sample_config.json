{
	"endpoint" : "http://localhost:8000/nginx_status",
	"metrics_config"  : [
		{ "name" : "activeConnections",  
		  "metric_type" : "gauge",
		  "units" : "number of active connections",
		  "data_type" : "int",
		  "polling_frequency" : 10,
		  "regex" : "Active connections: ([0-9]+)"			
		},
		{ "name" : "reading",
		  "metric_type" : "gauge", 
		  "units" : "number of reading connections",
		  "data_type" : "int",
		  "polling_frequency" : 10,
		  "regex" : "Reading: ([0-9]+) .*"	
		},
		{ "name" : "writing",
                  "metric_type" : "gauge",
                  "data_type" : "int",
		  "units" : "number of writing connections",
                  "polling_frequency" : 10,
                  "regex" : ".*Writing: ([0-9]+).*"
                },
		{ "name" : "waiting",
                  "metric_type" : "gauge",
                  "units" : "number of waiting connections",
		  "data_type" : "int",
                  "polling_frequency" : 10,
                  "regex" : ".*Waiting: ([0-9]+)"
                }
	] 	
	
}
