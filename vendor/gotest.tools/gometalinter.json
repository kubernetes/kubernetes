{
  "Vendor": true,
  "Deadline": "2m",
  "Sort": ["linter", "severity", "path"],
  "Exclude": [
    "example_test\\.go",
    "assert/cmd/gty-migrate-from-testify/testdata",
    "internal/difflib/difflib.go"
  ],

  "Enable": [
    "deadcode",
    "errcheck",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "gosimple",
    "ineffassign",
    "interfacer",
    "lll",
    "misspell",
    "unconvert",
    "unparam",
    "unused",
    "vet"
  ],

  "Cyclo": 10,
  "LineLength": 100
}
