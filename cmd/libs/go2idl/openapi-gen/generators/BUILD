    name = "go_default_library",
    srcs = ["openapi.go"],
    tags = ["automanaged"],
    deps = [
        "//vendor/github.com/golang/glog:go_default_library",
        "//vendor/k8s.io/apimachinery/pkg/openapi:go_default_library",
