apiVersion: v1
kind: Pod
metadata:
  name: agnhost
  labels:
    name: agnhost
spec:
  containers:
  - name: agnhost
    image: {{.AgnhostImage}}
    args: ["netexec", "--http-port=80"]
    ports:
    - containerPort: 80
    readinessProbe:
      httpGet:
        path: /
        port: 80
      initialDelaySeconds: 5
      timeoutSeconds: 5
